{
  "name": "vincent-p",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:vincent-p/www-vincent-p-fr.git"
  },
  "contributors": [
    "Vincent PECQUERIE <contact@vincent-p.fr>"
  ],
  "watch": {
    "lint:ts": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": true
    },
    "lint:html": {
      "patterns": [
        "src"
      ],
      "extensions": "html",
      "quiet": true
    },
    "lint:css": {
      "patterns": [
        "src"
      ],
      "extensions": "css,scss",
      "quiet": true
    }
  },
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "start": "ng serve",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "build": "ng build",
    "build:stats": "ng build --prod --stats-json",
    "build:client": "ng build --prod -c $ANGULAR_CONFIGURATION",
    "build:server-bundle": "ng run vincent-p:server:$ANGULAR_CONFIGURATION",
    "build:client-and-server-bundles": "npm run build:client && npm run build:server-bundle",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "dev:ssr": "ng build --prod -c beta && ng run vincent-p:server:beta && tsc -p server.tsconfig.json && node dist/server",
    "compile:server": "tsc -p server.tsconfig.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "serve:lint": "./node_modules/.bin/npm-watch",
    "serve:test": "ng test --browsers ChromeHeadless --reporters spec --watch true",
    "test": "ng test --browsers ChromeHeadless --reporters spec --watch false --code-coverage",
    "lint": "npm run lint:ts && npm run lint:html && npm run lint:css && npm run lint:docker",
    "lint:ts": "ng lint vincent-p",
    "lint:html": "./node_modules/.bin/htmllint src/app/**/**/**/**/**/**/**/**/**/*.html",
    "lint:css": "./node_modules/.bin/stylelint src/app/**/**/**/**/**/**/**/**/**/*.scss",
    "lint:docker": "./node_modules/.bin/dockerfile_lint",
    "build:doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d ./dist/documentation --theme material",
    "serve:doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -d ./dist/documentation --theme material -s -r 4201 -w"
  },
  "pre-commit": [],
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "0.803.0",
    "@angular/animations": "^8.2.3",
    "@angular/cdk": "^8.1.3",
    "@angular/cli": "8.3.0",
    "@angular/common": "^8.2.3",
    "@angular/compiler": "^8.2.3",
    "@angular/compiler-cli": "^8.2.3",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^8.2.3",
    "@angular/language-service": "^8.2.3",
    "@angular/material": "^8.1.3",
    "@angular/platform-browser": "^8.2.3",
    "@angular/platform-browser-dynamic": "^8.2.3",
    "@angular/platform-server": "^8.2.3",
    "@angular/pwa": "^0.803.0",
    "@angular/router": "^8.2.3",
    "@angular/service-worker": "^8.2.3",
    "@nguniversal/common": "^8.1.1",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "@nicky-lenaers/ngx-scroll-to": "^3.0.1",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.137",
    "bootstrap": "^4.3.1",
    "bootstrap-notify": "^3.1.3",
    "cerialize": "^0.1.18",
    "classlist.js": "^1.1.20150312",
    "compression": "^1.7.4",
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "express-minify-html": "^0.12.0",
    "font-awesome": "^4.7.0",
    "gulp": "^4.0.2",
    "gulp-purifycss": "^0.2.0",
    "gulp-sri-hash": "^2.2.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "ng-lazyload-image": "^6.1.0",
    "ngx-quill": "^7.2.0",
    "ngx-raven": "^1.0.5",
    "perfect-scrollbar": "^1.4.0",
    "popper.js": "^1.15.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "quill": "^1.3.6",
    "quill-emoji": "^0.1.7",
    "quill-mention": "^2.1.2",
    "raven-js": "^3.27.2",
    "reflect-metadata": "^0.1.13",
    "tooltip.js": "^1.3.2",
    "webfontloader": "^1.6.28",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.7.2",
    "codelyzer": "^5.1.0",
    "dockerfile_lint": "^0.3.3",
    "htmllint": "^0.8.0",
    "htmllint-cli": "0.0.7",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasminewd2": "^2.2.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.32",
    "npm-watch": "^0.6.0",
    "pre-commit": "^1.2.2",
    "protractor": "^5.4.2",
    "rxjs": "^6.5.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.4",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "webdriver-manager": "^12.1.6",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
