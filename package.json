    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@nicky-lenaers/ngx-scroll-to": "^1.1.1",
    "bootstrap": "^4.1.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "express": "^4.15.2",
